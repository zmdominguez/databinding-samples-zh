<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dev.zarah.databinding">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BannersActivity"
            android:label="Banners" />

        <activity android:name=".ClipImageActivity" />

        <activity android:name=".ProfileActivity" />

        <activity android:name=".StringFormattingActivity"
            android:label="@string/example_string_formatting"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ViewPager2DemoActivity"
            android:label="@string/viewpager2_listener"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".ViewPagerDemoActivity"
            android:label="@string/viewpager_listener"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>